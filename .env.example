# URL base do backend do PostaJá (não incluir barra no final)
BACK_BASE=https://back.clubepostaja.com.br

# Credenciais do PostaJá
PJ_EMAIL=seu-email@dominio.com.br
PJ_SENHA=sua-senha-aqui

# Porta do servidor Express
PORT=10000

# Caminho para cache local do token (persistente durante o ciclo de vida do container)
TOKEN_CACHE=./token-cache.json

# Intervalo mínimo entre requisições externas (ms). 1000 = 1 req/s.
RATE_MIN_INTERVAL_MS=1000
